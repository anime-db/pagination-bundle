{
  "name": "anime-db/pagination-bundle",
  "license": "MIT",
  "type": "library",
  "description": "Pagination bundle",
  "keywords": ["php", "pagination"],
  "homepage": "http://github.com/anime-db/pagination-bundle",
  "autoload": {
    "psr-4": {
      "AnimeDb\\Bundle\\PaginationBundle\\": "src/"
    }
  },
  "autoload-dev": {
