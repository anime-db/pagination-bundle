      "AnimeDb\\Bundle\\PaginationBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "symfony/dependency-injection": "2.8.*",
    "symfony/config": "2.8.*",
    "symfony/http-kernel": "2.8.*",
    "symfony/expression-language": "2.8.*",
    "doctrine/collections": "1.3.*"
  },
  "minimum-stability": "dev"
}
